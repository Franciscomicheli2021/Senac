package br.senac.tarefas;

public class tarefa09PesoIdeal {

	public static void main(String[] args) {
		double altura = 1.85;
		
		double pesoHomem = (72.7 * altura) - 58;
		double pesoMulher = (62.1 * altura) - 44.7;
		
		System.out.printf("Para o Homem com altura de "+ altura + " o peso ideal é " + pesoHomem);
		System.out.printf("\nPara a Mulher com altura de "+ altura + " o peso ideal é " + pesoMulher);

	}

}
