package br.senac.tarefas;

public class tarefa06G {

	public static void main(String[] args) {
		
		tarefa06FolhaDePagamento f = new tarefa06FolhaDePagamento();
		
		f.setSalario(25);
		f.setDesconto(0);
		f.setHorasTrabalhadas(160);
		
		double salarioFinal = f.getSalario() * f.getHorasTrabalhadas() - f.getDesconto();
		
		System.out.printf("%.2f", salarioFinal);

	}

}
