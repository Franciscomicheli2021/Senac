package br.senac.aula01;

public class ManipulaVariaveis {

	public static void main(String[] args) {
		boolean opcao = false;
		int numero = 1;
		String texto = "1";
		
		boolean resultado = opcao && !opcao;
		numero = numero + 2;
		//numero += 2;
		resultado = (numero>=2) || resultado;
		numero = 1;
		//resultado = (numero==texto)&&resultado;
		resultado = Integer.toString(numero).equals(texto) && resultado; //foi transformada a variavel numero em string e feito a compara√ßao.
		
		System.out.println(resultado);
	}

}
